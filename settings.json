{
    "workbench.colorTheme": "Monokai",
    "workbench.enableExperiments": false,
    "workbench.tree.indent": 13,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.list.openMode": "doubleClick",
    "workbench.startupEditor": "none",
    "workbench.activityBar.visible": false,
    "workbench.settings.editor": "json",
    "breadcrumbs.enabled": false,
    "editor.fontSize": 14,
    "editor.fontFamily": "Consolas, 'Noto Sans JP'",
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.tabCompletion": "on",
    "editor.formatOnType": true,
    "editor.formatOnPaste": false,
    "editor.wordWrap": "on",
    "editor.minimap.showSlider": "always",
    "editor.dragAndDrop": false,
    "editor.hover.enabled": false,
    "editor.minimap.renderCharacters": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.includeStrings": false,
    "window.restoreWindows": "all",
    "window.menuBarVisibility": "toggle",
    "window.newWindowDimensions": "maximized",
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.autoReveal": false,
    "explorer.confirmDragAndDrop": false,
    "update.showReleaseNotes": false,
    "update.enableWindowsBackgroundUpdates": false,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.banner": "always",
    "files.hotExit": "off",
    "files.simpleDialog.enable": true,
    "files.insertFinalNewline": true,
    "debug.javascript.codelens.npmScripts": "never",
    "git.enabled": false,
    "git.ignoreMissingGitWarning": true,
    "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    "npm.fetchOnlinePackageInfo": false,
    "extensions.ignoreRecommendations": false,
    "csscomb.preset": "C:/Users/owner/.vscode/extensions/mrmlnc.vscode-csscomb-5.3.2/node_modules/csscomb/config/csscomb.json",
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "markdown.extension.print.absoluteImgPath": false,
    "markdown.extension.list.indentationSize": "inherit",
    "settingsSync.ignoredSettings": [
        "editor.fontSize"
    ],
    "settingsSync.keybindingsPerPlatform": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.pytest_cache": true
    },

    "html.autoClosingTags": false,
    "htmltagwrap.tag": "strong",
    "htmltagwrap.autoDeselectClosingTag": false,

    "typescript.updateImportsOnFileMove.enabled": "never",

    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
    ],
    "eslint.format.enable": true,

    "prettier.tabWidth": 2,
    "prettier.bracketSpacing": false,
    "prettier.printWidth": 777,
    "prettier.semi": true,

    "volar.codeLens.references": false,
    "volar.icon.splitEditors": false,
    "volar.completion.preferredAttrNameCase": "camel",
    "volar.completion.preferredTagNameCase": "pascal",
    "volar.autoCompleteRefs": true,

    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django",
    ],
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.stubPath": "functions/typings",
    "python.analysis.autoImportCompletions": false,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportUnusedImport": "none",
        "reportUnusedClass": "information",
        "reportUnusedFunction": "none",
        "reportUnboundVariable": "warning",
        "reportUnknownMemberType": "none",
        "reportMissingModuleSource": "information",
        "reportMissingTypeStubs": "information",
        "reportInvalidStringEscapeSequence": "none",
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left",
    },

    "php.validate.run": "onType",
    "phpserver.browser": "",
    "php-cs-fixer.executablePath": "${extensionPath}\\php-cs-fixer.phar",
    "intelephense.stubs": ["apache", "bcmath", "bz2", "calendar", "com_dotnet", "Core", "ctype", "curl", "date", "dba", "dom", "enchant", "exif", "FFI", "fileinfo", "filter", "fpm", "ftp", "gd", "gettext", "gmp", "hash", "iconv", "imap", "intl", "json", "ldap", "libxml", "mbstring", "meta", "mysqli", "oci8", "odbc", "openssl", "pcntl", "pcre", "PDO", "pdo_ibm", "pdo_mysql", "pdo_pgsql", "pdo_sqlite", "pgsql", "Phar", "posix", "pspell", "readline", "Reflection", "session", "shmop", "SimpleXML", "snmp", "soap", "sockets", "sodium", "SPL", "sqlite3", "standard", "superglobals", "sysvmsg", "sysvsem", "sysvshm", "tidy", "tokenizer", "xml", "xmlreader", "xmlrpc", "xmlwriter", "xsl", "Zend OPcache", "zip", "zlib", "wordpress"],

    "rust-analyzer.lens.run.enable": false,
    "rust-analyzer.lens.debug.enable": false,
    "rust-analyzer.lens.implementations.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,

    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.validate": false,

    "cfnLint.validateUsingJsonSchema": true,

    "[plaintext]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
    },
    "[markdown]":  {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        },
        "editor.suggest.showKeywords": false,
        "editor.suggest.showWords": false,
    },
    "[html]": {
        "editor.tabSize": 2,
    },
    "[css]": {
    },
    "[scss]": {
        "editor.tabSize": 2,
    },
    "[javascript]": {
        "editor.tabSize": 2,
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.typescript-language-features",
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": false,
    },
    "[vue]": {
        "editor.tabSize": 2,
    },
    "[python]": {
        "editor.semanticHighlighting.enabled": false,
    },
    "[tf]": {
        "editor.tabSize": 2,
    },
}
